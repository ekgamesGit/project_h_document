<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="Root">
    <xs:complexType>
      <xs:sequence>
<!-- gameConfig Table-->
        <xs:element name="gameConfig" minOccurs="1" maxOccurs="unbounded">
          <xs:complexType>
 		<xs:attribute name="configName" type="xs:integer"/>
		<xs:attribute name="configValue" type="xs:string"/>

          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>